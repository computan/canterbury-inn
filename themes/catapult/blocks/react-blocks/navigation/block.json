{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "catapult/navigation",
	"version": "2.4.0",
	"title": "Navigation",
	"category": "navigation",
	"icon": "",
	"description": "The primary navigation block.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"style": {
			"type": "string",
			"default": "full-width-dropdown"
		},
		"logoUrl": {
			"type": "string"
		},
		"logoAlt": {
			"type": "string"
		},
		"logoWidth": {
			"type": "number"
		},
		"logoHeight": {
			"type": "number"
		},
		"logoId": {
			"type": "number"
		}
	},
	"styles": [
		{
			"name": "full-width-dropdown",
			"label": "Full-Width-Dropdown",
			"isDefault": true
		},
		{
			"name": "variable-width-dropdown",
			"label": "Variable-Width-Dropdown"
		},
		{
			"name": "hamburger",
			"label": "Hamburger"
		}
	],
	"providesContext": {
		"catapult/navigation/style": "style"
	},
	"textdomain": "catapult",
	"viewScript": "file:./view.js",
	"editorScript": "file:./index.js",
	"style": "file:./style.css",
	"editorStyle": "file:./editor.css"
}